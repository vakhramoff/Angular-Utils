{
  "name": "@vakhramoff/angular-utils",
  "author": {
    "name": "Sergey Vakhramov",
    "url": "https://vakhramoff.ru"
  },
  "version": "14.0.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vakhramoff/Angular-Utils.git"
  },
  "keywords": [
    "Angular",
    "Utilities",
    "Angular Utilities",
    "Angular Utils",
    "TypeScript",
    "Error Handling",
    "HTTP Error",
    "HTTP Error Handling",
    "Decorator",
    "Service",
    "Directive",
    "Dependency Injection",
    "Injection Token"
  ],
  "scripts": {
    "build:lib": "ng build angular-utils --configuration production",
    "build:pack": "npm run build:lib && npm pack",
    "publish:major": "npm version major && npm run build:lib && cd ../../dist/angular-utils && npm publish",
    "publish:minor": "npm version minor && npm run build:lib && cd ../../dist/angular-utils && npm publish",
    "publish:patch": "npm version patch && npm run build:lib && cd ../../dist/angular-utils && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@angular/common": "^14.0.1",
    "@angular/core": "^14.0.1"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  }
}
